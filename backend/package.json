  "description": "A events management portal for Jain Universtiy",
  "main": "index.ts",
  "scripts": {
    "start": "pnpx nodemon ./index.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "pnpx nodemon --exec pnpx ts-node ./index.ts",
    "debug": "pnpx nodemon --inspect --exec pnpx ts-node ./index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeryjs/Jain-Events-Portal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeryjs/Jain-Events-Portal/issues"
  },
  "homepage": "https://github.com/jeryjs/Jain-Events-Portal#readme",
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express-rate-limit": "^7.5.1",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "node-cache": "^5.1.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
