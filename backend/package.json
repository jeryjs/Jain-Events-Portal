    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.16.0",
    "express": "^4.21.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@common": "../common",
    "@config": "./config",
    "@middlewares": "./middlewares",
    "@routes": "./routes",
    "@services": "./services",
    "@utils": "./utils"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
